{
  "name": "nice-react-icon",
  "version": "2.1.1",
  "description": "A flexible and customizable React icon component with built-in icon set and theming support",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:niceprototypes/nice-react-icon.git"
  },
  "homepage": "https://github.com/niceprototypes/nice-react-icon#readme",
  "bugs": {
